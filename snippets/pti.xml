<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="pti"
           revision="10.0"
           fetch="https://github.com/Project-Titanium/"
           sync-j="8" />

  <!-- Removal Xml -->
  <include name="snippets/remove.xml" />


  <project path="build/make" name="build" groups="pdk" remote="pti" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <project path="build/soong" name="build_soong"  remote="pti" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  
  <project path="bionic" name="bionic" remote="pti" />
  <project path="frameworks/base" name="frameworks_base" remote="pti" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="pti" />
  <project path="vendor/pti" name="vendor_pti" remote="pti" />

</manifest>
